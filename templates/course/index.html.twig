{# templates/product/index.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Hello Budy !
{% endblock %}

{% block body %}

		{% for course in courses %}
		<div class="col-m-4 font_body">
            <div class="card">
                <div class="card-body">
					<h3 class="card-title" style="text-align: center;">{{ course.name }}</h3>
					<div class="card-text" style="text-align: center;">
						<img src="uploads/{{ course.image }}" alt="">
					</div>
				</div>
			</div>
		</div>
		<br>

					
		{% endfor %} 
	
	{% endblock %}
